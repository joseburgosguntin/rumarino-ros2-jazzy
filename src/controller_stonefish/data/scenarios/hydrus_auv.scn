			<thrust_model type="fluid_dynamics">
				<thrust_coeff forward="0.48" reverse="0.48" />
				<torque_coeff value="0.05" />
			</thrust_model>
		</actuator>

		<actuator name="thruster_back_left" type="thruster">
			<link name="Hydrus" />
			<origin rpy="0.0 0.0 -1.9635" xyz="-0.198 0.407 0.0" />
